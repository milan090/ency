{
  "name": "ency",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:watch": "concurrently --kill-others --names \"NEXT,NEXUS,GQL\" -c \"blue,green,magenta\" \"yarn dev\" \"yarn generate:nexus:watch\" \"yarn graphql-codegen --watch\"",
    "prebuild": "yarn generate",
    "build": "next build",
    "start": "next start",
    "generate": "yarn run generate:prisma && yarn run generate:nexus && yarn graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env ts-node -r dotenv/config -r tsconfig-paths/register --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "generate:watch": "yarn run generate:prisma && yarn run generate:nexus:watch",
    "generate:nexus:watch": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env ts-node-dev --respawn -r dotenv/config -r tsconfig-paths/register --transpile-only -P nexus.tsconfig.json src/server/graphql/schema.ts",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.21.0",
    "@prisma/client": "2.21.2",
    "@sendgrid/mail": "^7.4.4",
    "@tailwindui/react": "^0.1.1",
    "@urql/devtools": "^2.0.3",
    "apollo-server-micro": "^2.24.0",
    "autoprefixer": "^10.2.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.2.2",
    "next-auth": "^3.19.3",
    "next-connect": "^0.10.1",
    "next-seo": "^4.24.0",
    "next-urql": "^3.0.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-editor-js": "^1.9.0",
    "react-feather": "^2.0.9",
    "react-hook-form": "^6.14.1",
    "react-hot-toast": "^1.0.2",
    "react-outside-click-handler": "^1.3.0",
    "react-query": "^3.13.6",
    "sass": "^1.32.8",
    "tailwindcss": "^2.0.2",
    "ts-node-dev": "^1.1.6",
    "urql": "^2.0.2",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@editorjs/code": "^2.7.0",
    "@editorjs/embed": "^2.4.6",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.0",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/raw": "^2.3.0",
    "@editorjs/simple-image": "^1.4.1",
    "@editorjs/table": "^1.3.0",
    "@editorjs/warning": "^1.2.0",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/near-operation-file-preset": "^1.18.0",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-urql": "^2.0.6",
    "@prisma/studio": "^0.397.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/classnames": "^2.3.1",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.3",
    "@types/react-outside-click-handler": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prisma": "2.21.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma"
  }
}
